{
    "name": "amorebietakoudala/mipago-bundle",
    "description": "A bundle to make payment through Basque Goverment's MiPago platform in symfony projects",
    "type": "symfony-bundle",
    "authors": [{
        "name": "Amorebietako Udala",
        "email": "informatika@amorebieta.eus"
    }],
    "keywords": [
        "Payments MiPago Basque Government"
    ],
    "license": [
        "MIT"
    ],
    "require": {
        "php": ">=8.2.0",
        "doctrine/orm": "^2.5",
        "symfony/translation": "^6.4",
        "twig/twig": "^2.12|^3.0",
        "symfony/config": "^6.4",
        "symfony/dom-crawler": "^6.4",
        "symfony/framework-bundle": "^6.4",
        "symfony/http-foundation": "^6.4",
        "symfony/http-kernel": "^6.4"
    },
    "autoload": {
        "psr-4": {
            "MiPago\\Bundle\\": ""
        }
    },
    "repositories": [{
        "type": "vcs",
        "url": "https://github.com/amorebietakoUdala/mipago-bundle"
    }],
    "scripts": {
        "post-install-cmd": []
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require-dev": {
        "rector/rector": "^0.18.12"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.4.*",
            "docker": false
        }
    }    
}
