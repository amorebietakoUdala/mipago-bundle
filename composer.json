{
    "name": "amorebietakoudala/mipago-bundle",
    "description": "A bundle to make payment through Basque Goverment's MiPago platform in symfony projects",
    "type": "symfony-bundle",
    "authors": [{
        "name": "Amorebietako Udala",
        "email": "informatika@amorebieta.eus"
    }],
    "keywords": [
        "Payments MiPago Basque Government"
    ],
    "license": [
        "MIT"
    ],
    "require": {
        "php": ">=7.1.0",
        "doctrine/orm": "^2.5",
        "symfony/translation": "^4.0|^5.0",
        "twig/twig": "^2.12|^3.0",
        "symfony/config": "^4.0|^5.0",
        "sensio/framework-extra-bundle": "^4.0|^5.0|^6.0",
        "symfony/http-foundation": "^4.0|^5.0.7",
        "symfony/dom-crawler": "^4.0|^5.0",
        "symfony/http-kernel": "^5.1.5|^5.3"
    },
    "autoload": {
        "psr-4": {
            "MiPago\\Bundle\\": ""
        }
    },
    "repositories": [{
        "type": "vcs",
        "url": "https://github.com/amorebietakoUdala/mipago-bundle"
    }],
    "scripts": {
        "post-install-cmd": []
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}